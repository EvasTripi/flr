        }
    },
    {
        "model": "FlrMenu",
        "meta_id": "menu_flruser",
        "data": {
            "name": "Users",
            "sequence": 1,
            "section_id_META_ID": "menu_section_admin",
            "groups_META_ID": ["flrgroup_admin"]
        }
    },
    {
        "model": "FlrView",
        "meta_id": "flruser_list_view",
        "data": {
            "name": "Users list view",
            "model": "FlrUser",
            "view_type": "list",
            "definition": {
                "create": true,
                "edit": true,
                "structure": [
                    {"field": "name"},
                    {"field": "email"}
                ]
            },
            "menu_id_META_ID": "menu_flruser",
            "sequence": 1
        }
    },
    {
        "model": "FlrView",
        "meta_id": "flruser_form_view",
        "data": {
            "name": "Users form view",
            "model": "FlrUser",
            "view_type": "form",
            "definition": {
                "buttons": [
                    {
                        "text": "_('Change password')",
                        "action": "openViews",
                        "options": {
                            "model": "FlrUserChangePassword",
                            "view_types": ["form"],
                            "globals": {"user_id": "this.id"},
                            "saveButton": true,
                            "class": "btn-primary"
                        }
                    }
                ],
                "structure": [
                    {"section": "_('General')"},
                    {"section": "_('Roles')"},
                    {"section": "_('Allowed companies')"}
                ],
                "_('General')": [
                    {"field": "name"},
                    {"field": "email"},
                    {"field": "company_id"},
                    {"field": "active"},
                    {"field": "lang"},
                    {"field": "email_notifications"}
                ],
                "_('Roles')": [
                    {
                        "label": "Roles",
                        "field": "groups",
                        "options": {
                            "add": true,
                            "remove": true,
                            "related_fields": [{"field": "name"}]
                        }
                    }
                ],
                "_('Allowed companies')": [
                    {
                        "label": "Allowed companies",
                        "field": "allowed_companies",
                        "options": {
                            "add": true,
                            "remove": true,
                            "related_fields": [{"field": "name"}]
                        }
                    }
                ]
            },
            "menu_id_META_ID": "menu_flruser",
            "sequence": 2
        }
    },
    {
        "model": "FlrMenu",
        "meta_id": "menu_flrgroup",
